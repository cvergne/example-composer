{
	"name": "php-actions/example-composer",
	"description": "An example project that uses php-actions/composer",

	"require": {
		"phpgt/webengine": "^v2"
	},

	"autoload": {
		"psr-4": {
			"ExampleComposer\\": "./src"
		}
	},

	"scripts": {
		"env-test": "ExampleComposer\\Debug::inspectEnvironmentVariables",
		"memorylimit-test": "ExampleComposer\\Debug::inspectMemoryLimit",
		"post-install-cmd": [
			"ExampleComposer\\Debug::inspectEnvironmentVariables",
			"ExampleComposer\\Debug::inspectMemoryLimit"
		]
	}
}
